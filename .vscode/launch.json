    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome - Angular Dev Server",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/carla-maid",
            "preLaunchTask": "start-angular-dev-server"
        },
        {
            "name": "Launch Chrome - Production Server",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4000",
            "webRoot": "${workspaceFolder}/carla-maid",
            "preLaunchTask": "start-production-server"
        }
    ]
}