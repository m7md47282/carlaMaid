{
  "name": "carla-maid",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:carla-maid": "node dist/carla-maid/server/server.mjs",
    "backend": "node standalone-server.js",
    "backend:dev": "NODE_ENV=development PORT=4000 node standalone-server.js",
    "backend:prod": "NODE_ENV=production PORT=3000 node standalone-server.js",
    "backend:start": "./start-backend.sh",
    "dev": "concurrently \"npm run backend:dev\" \"npm run start\"",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run start\" \"npm run watch\"",
    "create:production": "./deploy-production.sh",
    "build:production": "npm run build && npm run create:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.12",
    "@angular/cdk": "^18.2.9",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/material": "^18.2.9",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/platform-server": "^18.2.12",
    "@angular/pwa": "^0.5.3",
    "@angular/router": "^18.2.12",
    "@angular/service-worker": "^18.2.13",
    "@angular/ssr": "^18.2.12",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "axios": "^1.11.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "gsap": "^3.12.5",
    "primeng": "^17.18.11",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "concurrently": "^8.2.2",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}
