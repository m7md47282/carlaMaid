{
  "name": "carla-maid-payment-success-tests",
  "version": "1.0.0",
  "description": "Comprehensive payment success test suite for Carla Maid application",
  "main": "run-payment-success-tests.js",
  "scripts": {
    "test": "node run-payment-success-tests.js",
    "test:backend": "node run-payment-success-tests.js --backend-only",
    "test:frontend": "node run-payment-success-tests.js --frontend-only",
    "test:headless": "node run-payment-success-tests.js --headless",
    "test:quick": "node run-payment-success-tests.js --no-details --no-save",
    "test:specific": "node run-payment-success-tests.js --specific",
    "test:backend-only": "node test-payment-success.js",
    "test:frontend-only": "node test-payment-success-frontend.js",
    "install-deps": "npm install axios puppeteer",
    "setup": "npm run install-deps && echo 'Test setup complete!'"
  },
  "keywords": [
    "payment",
    "success",
    "testing",
    "mock",
    "carla-maid",
    "skipcash",
    "angular",
    "nodejs"
  ],
