  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "express": "^4.18.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carla-maid/payment-success-tests.git"
  },
  "bugs": {
